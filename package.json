{
  "name": "hellobooks-application",
  "version": "1.0.0",
  "description": "Hello Books Application clientside frontend",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel lint:watch start:server",
    "open:client": "babel-node tools/clientServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* client tools server",
    "lint:watch": "npm run lint -- --watch",
    "start:server": "set NODE_ENV=development&& babel-node  ./server/bin/www.js --exec babel-node --presets env ",
    "start:test": "set NODE_ENV=test&& babel-node  ./server/bin/www.js --exec babel-node --presets env ",
    "test": "nyc mocha server/test/* --timeout 15000 --compilers js:babel-register",
    "initialize:db": "babel-node server/scripts/initialize.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls ",
    "codeclimate:report": "recink run unit -c codeclimate"
  },
  "author": "Paradise Kelechi",
  "license": "MIT",
