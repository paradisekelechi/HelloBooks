  "scripts": {
    "start": "NODE_ENV='production' webpack && babel-node  app.js",
    "start:dev": "NODE_ENV=development webpack -d && npm run copy:assets && babel-node  app.js",
    "start:server": "NODE_ENV=development babel-node  app.js",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha test/server --timeout 150000 --compilers js:babel-register",
    "copy:assets": "cp -r ./client/assets ./dist/. && cp ./client/index.html ./dist/index.html",
    "lint": "node_modules/.bin/esw webpack.config.* client tools server",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run test -- --watch",
    "seed:db": "NODE_ENV=test babel-node server/scripts/initialize.js",
    "coverage": "nyc report --reporter=text --reporter=lcov --reporter=html | coveralls"
